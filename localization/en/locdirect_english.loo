		},
		LocDirectEntry
		{
			Key = "$action.WriteTrashLetterDesc";
			Text = "You simply cannot stand someone? Then just write a defamatory pamphlet about that person and distribute it around the city...";
		},
		LocDirectEntry
		{
			Key = "$action.WriteTrashLetterInstr";
			Text = "Select the person about whom you want to write a pamphlet";
